{
  "rules": {
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "radix": 2,
    "no-eq-null": 0,
    "quotes": [2, "single"],
    "no-else-return": 1,
    "block-scoped-var": 2,
    "no-lonely-if": 1,
    "comma-style": 0,
    "no-nested-ternary": 1,
    "curly": 2,
    "camelcase": 0,
    "no-undef": 2,
    "no-multi-spaces": 2,
    "wrap-iife": [2, "inside"],
    "generator-star-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "no-div-regex": 1,
    "strict": 0,
    "space-after-keywords": [2, "always"],
    "quote-props": [2, "as-needed"],
    "no-process-exit": 0,
    "no-new-require": 2,
    "no-mixed-requires": 2,
    "no-debugger": 1,
    "no-floating-decimal": 2,
    "no-loop-func": 1,
    "no-unused-vars": [2, { "vars": "all", "args": "after-used" }],
    "space-before-blocks": [2, "always"],
    "no-trailing-spaces": 2,
    "space-before-function-paren": [2, "always"],
    "no-extra-parens": 2,
    "no-path-concat": 2,
    "semi": [2, "never"],
    "global-strict": 0,
    "new-cap": 0,
    "no-catch-shadow": 2,
    "handle-callback-err": 0,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "eqeqeq": [2, "smart"],
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "new-parens": 0
  },
  "env": {
    "node": true,
    "es6": true
  }
}
